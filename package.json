{
  "name": "handvas",
  "version": "0.0.1",
  "description": "Hand drawing on a javascript canvas",
  "source": "src/index.ts",
  "main": "build/main.js",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "module": "build/module.js",
  "types": "build/types.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src --ext .ts",
    "lint:quiet": "eslint ./src --ext .ts --quiet",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "check": "tsc --noEmit",
    "watch": "parcel watch",
    "start": "parcel example/index.html --dist-dir ./build",
    "build": "rm -rf ./build && parcel build --no-source-maps --dist-dir ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlessandroCipolletti/Handvas.git"
  },
  "keywords": [
    "hand",
    "drawings",
    "drawing",
    "canvas",
    "library",
    "javascript"
  ],
  "author": "Alessandro Cipolletti <cipolletti.alessandro@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlessandroCipolletti/Handvas/issues"
  },
  "homepage": "https://github.com/AlessandroCipolletti/Handvas#readme",
  "dependencies": {
    "@types/debounce": "^1.2.1",
    "debounce": "^1.2.0",
    "mobile-device-detect": "^0.4.3"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "parcel": "^2.8.3",
    "typescript": "^4.9.5"
  }
}
