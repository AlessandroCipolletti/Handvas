{
  "name": "handvas",
  "version": "0.0.1",
  "description": "Hand drawing javascript canvas library",
  "source": "src/index.ts",
  "main": "build/main.js",
  "module": "build/module.js",
  "types": "build/types.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src --ext .ts",
